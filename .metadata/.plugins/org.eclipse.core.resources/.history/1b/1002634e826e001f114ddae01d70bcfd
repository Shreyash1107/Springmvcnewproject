function validateBatchForm() {
    return validatePassoutYear();
}

function validatePassoutYear() {
    var currentYear = new Date().getFullYear();
    var passoutYear = document.getElementById('passoutYear').value;
    if (passoutYear > currentYear) {
        alert("Passout year cannot be in the future.");
        return false; 
    }
    return true; 
}
